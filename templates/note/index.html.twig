<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>
<body>

<table border="2">

        {% for note in notes %}
    <tr><td>{{ note }}</td></tr>
        {% endfor %}

</table>

<div>
    <p>Min = {{ min(notes) }}</p>
    <p>Max = {{ max(notes) }}</p>
    <p>Taille = {{ notes | length }}</p>
</div>

</body>
</html>